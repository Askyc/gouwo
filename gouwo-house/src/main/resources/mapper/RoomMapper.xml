<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gouwo.mapper.RoomMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gouwo.model.RoomModel">
        <id column="room_id" property="roomId" />
        <result column="room_num" property="roomNum" />
        <result column="household" property="household" />
        <result column="acreage" property="acreage" />
        <result column="towards" property="towards" />
        <result column="floor" property="floor" />
        <result column="area" property="area" />
        <result column="address_id" property="addressId" />
        <result column="features_id" property="featuresId" />
        <result column="house_type" property="houseType" />
        <result column="period" property="period" />
        <result column="homeowner" property="homeowner" />
        <result column="rental_status" property="rentalStatus" />
        <result column="current_tenant" property="currentTenant" />
        <result column="current_rent" property="currentRent" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        room_id, room_num, household, acreage, towards, floor, area, address_id, features_id, house_type, period, homeowner, rental_status, current_tenant, current_rent
    </sql>

</mapper>

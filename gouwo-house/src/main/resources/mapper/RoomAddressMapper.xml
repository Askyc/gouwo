<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gouwo.mapper.RoomAddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gouwo.model.RoomAddressModel">
        <id column="address_id" property="addressId" />
        <result column="room_id" property="roomId" />
        <result column="province" property="province" />
        <result column="city" property="city" />
        <result column="zone" property="zone" />
        <result column="detailed" property="detailed" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        address_id, room_id, province, city, zone, detailed
    </sql>

</mapper>
